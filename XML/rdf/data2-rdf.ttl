    <http://example.org/whiskeyman/customer/2> a ex:Customer ;
        ex:cusomerId        1 ;
        schema:name         "Matyas Brabec" ;
        schema:givenName    "Matyas" ;
        schema:familyName   "Brabec" ;
        schema:email        "Stooped@hotmail.com" ;
        ex:login            "KutalejiciseKrava" .
        
        <http://example.org/whiskeyman/customer/2> ex:nfcTag <http://example.org/whiskeyman/customer/nfcTag/2> .
        <http://example.org/whiskeyman/customer/nfcTag/2> a ex:customer, ex:nfcTag ;
                schema:text             "OXPR456ADS75" ;
                schema:purchaseDate     "2020-06-09T23:58:01"^^xsd:dateTime .
    
        <http://example.org/whiskeyman/customer/2> ex:Transaction <http://example.org/whiskeyman/customer/transaction/2> .
        <http://example.org/whiskeyman/customer/transaction/2> a ex:Customer, ex:Transaction ;
                ex:bottleID             1 ;
                ex:AmmountMl            1000 ;
                schema:Price            69000 ;
                cerif:currency          "CZK" ;
                schema:purchaseDate     "2021-01-12T12:00:00"^^xsd:dateTime .
    
        <http://example.org/whiskeyman/customer/2> ex:Ownership <http://example.org/whiskeyman/customer/ownership/2> .
        <http://example.org/whiskeyman/customer/ownership/2> a ex:Bottle, ex:Ownership ;
                ex:Share        50 ;
                ex:bottleID     4 .
    
